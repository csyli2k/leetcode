# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def detectCycle(self, head: Optional[ListNode]) -> Optional[ListNode]:
        seens=set()
        index=0
        while head:
            if head in seens:
                return head
            seens.add(head)
            head=head.next
        return None
            