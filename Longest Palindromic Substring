    def longestPalindrome(self, s: str) -> str:
        def is_pali(l,r):
            while l >=0 and r<=len(s)-1 and s[l]==s[r]:
                l-=1
                r+=1
            return s[l+1:r]
        res=""
        for i in range(len(s)):
            t1=is_pali(i,i)
            t2=is_pali(i,i+1)
            res=max(res,t1,t2, key=len)
        return res
        